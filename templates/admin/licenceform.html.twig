{% extends "admin/base.html.twig" %}

{% block main %}


	<div class="page-title">
		<h2>Ajouter une licence</h2>
	</div>

	<div class="container d-flex justify-content-center">
		<div class="card" style="width: 50%;">
			<img src="https://picsum.photos/450/250" class="card-img-top" alt="...">
			<div class="card-body">
				{{form_start(licenceForm)}}

				{{form_label(licenceForm.name, 'Nom')}}
				{{form_widget(licenceForm.name)}}
				<hr>

				{{form_label(licenceForm.description, 'Description')}}
				{{form_widget(licenceForm.description)}}
				<hr>

				{{form_label(licenceForm.media, 'Image')}}
				{{form_widget(licenceForm.media)}}
				<hr>

				{{form_widget(licenceForm.Enregistrer)}}

				{{form_end(licenceForm)}}
			</div>
		</div>
	</div>

{% endblock %}
