{% extends "admin/base.html.twig" %}

{% block main %}
	{% for image in images %}
		<div class="container d-flex justify-content-center">
			<div class="card" style="width: 50%;">
				<img src="{{ asset("images/" ~ image.src) }}" alt="{{ image.alt }}">
				<div class="card-body">
					<h4>{{ image.title }}</h4>
				</div>
			</div>
		</div>
	{% endfor %}

{% endblock %}
